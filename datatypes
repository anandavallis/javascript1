    <script>
        //String Method
        
        var a="Banana"
        console.log(a);

        // console.log(typeof(a));  //We want to know what method this is

        //Number Method
        let age=20;
        console.log(age);

        // console.log(typeof(age)); //We want to know what method this is
        
        //Boolean Method
        var a=true
        console.log(a)

        // console.log(typeof(a)); //We want to know what method this is
    </script>

