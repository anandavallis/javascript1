<body>
    <script>
        var name ="Anandavalli";  //string
        var age =20;      //number
        var isStudent =true; //boolean
 
        console.log("Name:",name);
        console.log("Age:",age);
        console.log("isStudent:",isStudent);

        //Modify Variables
        name = "Dory";
        age = 30;
        isStudent = false;

       console.log("Modify Name:", name);
       console.log("Modify Age:", age);
       console.log("Modify Is Student:", isStudent);

    </script>
</body>
</html>
