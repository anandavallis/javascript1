       //Subtraction
       var a = 20;
       var b = 10;
       console.log(a-b);
        
       //Multiplication
       var a = 10;
       var b = 20;
       console.log(a*b);

       //Division
       var a = 20;
       var b = 10;
       console.log(a/b);
    </script>
